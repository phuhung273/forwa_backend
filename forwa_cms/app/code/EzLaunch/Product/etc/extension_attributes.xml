<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Api/etc/extension_attributes.xsd">
    <extension_attributes for="Magento\Catalog\Api\Data\ProductInterface" >
        <attribute code="base_image_urls" type="string[]"/>
        <attribute code="quantity" type="int" />
        <attribute code="seller_name" type="string" />
        <attribute code="is_disabled" type="boolean" />
        <attribute code="latitude" type="string" />
        <attribute code="longitude" type="string" />
    </extension_attributes>
</config>
